package com.app.marketstarterDemo.MarketStarterDemo;

import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RestController;

@SpringBootApplication

@RestController

public class MarketStarterDemoApplication {

	public static void main(String[] args) {
		SpringApplication.run(MarketStarterDemoApplication.class, args);
	}

	
	@RequestMapping(name"/mybank")
	public String getRestoutput(String name) {
		String message="My name is"+name;
		System.out.println("Message="+message);
		return message;
	}
}
